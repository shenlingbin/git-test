<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.logistics_system.dao.IOrderDao">

    <insert id="add" parameterType="com.example.logistics_system.pojo.Orders">
        insert into orders(productNumber,productName,productUnit,orderUser,deliverman)
        VALUES (#{productNumber},#{productName}, #{productUnit},#{orderUser},#{deliverman})
    </insert>

</mapper>